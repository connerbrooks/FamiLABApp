<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="11" android:versionName="2.1" package="org.familab.app">

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="16"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@android:style/Theme.Holo.Light.DarkActionBar">
        <activity android:label="@string/app_name" android:name="org.familab.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="Settings" android:name="org.familab.app.Settings">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="org.familab.app.MainActivity"/>
        </activity>
        <activity android:label="@string/app_name" android:name="org.familab.app.WebViewActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="org.familab.app.Settings"/>
        </activity>
        <activity android:label="@string/title_activity_unique_item" android:name="org.familab.app.UniqueItem" android:parentActivityName="org.familab.app.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="org.familab.app.MainActivity"/>
        </activity>
        <activity android:label="@string/title_activity_area" android:name="org.familab.app.AreaActivity" android:parentActivityName="org.familab.app.MainActivity" android:theme="@style/Theme.TranslucentActionBar.ActionBar.Overlay">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="org.familab.app.MainActivity"/>
        </activity>
        <activity android:label="@string/title_activity_item_activtity" android:name="org.familab.app.ItemActivity" android:parentActivityName="org.familab.app.AreaActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="org.familab.app.AreaActivity"/>
        </activity>
    </application>

</manifest>
